{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "EventBus": "amqp://localhost"
  },
  "EventBus": {
    "SubscriptionClientName": "Apply"
  },
  "Identity": {
    "Audience": "apply",
    "Scopes": {
      "apply": "Apply API"
    }
  },
  "Aspire": {
    "Npgsql": {
      "EntityFrameworkCore": {
        "PostgreSQL": {
          "DbContextPooling": false,
          "HealthChecks": false,
          "Tracing": false
        }
      }
    }
  },
  "AllowedHosts": "*"
}
